@{
    string[] logs = ViewBag.EmployeeLogs;
}

<h1>Employee Logs</h1>
<a href="@Url.Action("SendEmail", "Home")" class="btn btn-primary">Wyslij zgloszenie</a>

@if (logs != null && logs.Length > 0)
{
    <ul>
        @foreach (var log in logs)
        {
            <li>@log</li>
        }
    </ul>
}
else
{
    <p>No logs available.</p>
}








